#Kivy(2.0.0)

<DivisorCuenta>:
    f_cantidad: cantPersonas
    btn_listar: listar
    GridLayout:
        cols: 2
        padding: 10
        spacing: 10
        
        Label:
            text: 'Cantidad de personas:'
        TextInput:
            id: cantPersonas
            multiline: True
            password: False
   
    GridLayout:
        rows: 1
        padding: 50
        Button:
            id: listar
            text: 'listar'
            on_press: root.calcularPersonas(cantPersonas)
            backgound_color: [0.5,0.5,0.5,0.5]
